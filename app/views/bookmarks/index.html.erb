<div class="container">
  <div class="row">
    <h1>All Bookmarks</h1>
    <h4>Like or Unlike Bookmarks</h4>
    <% @bookmarks.each do |bookmark| %>
    <h4><%= link_to "#{bookmark.url}", bookmark.url %>
      <%= render partial: 'likes/like', locals: { bookmark: bookmark } %></h4>
      <% end %>
      <%= link_to "My Bookmarks", bookmarks_my_bookmarks_path, class: 'btn btn-info btn-bottom' %> <%= link_to "View Topics", topics_path, class: 'btn btn-info btn-bottom' %>
    </div>
  </div>
